<!-- Form for inputting user information -->
    <div class="container"> 
    <div class="card-panel teal lighten-7 grid-example">
    <div class="row">
      <form class="col s12">
        <div class="row">
<h4>Your User Info:</h4> 
<!--Text Area to Input First name -->  
          <div class="input-field col s6">
            <i class="material-icons prefix">account_circle</i>
            <input id="icon_prefix" type="text" class="validate" value={{ first_name}}>
            <label for="icon_prefix">First Name</label>
          </div>
<!---Text Area to Input Last name -->          
         <div class="input-field col s6">
            <i class="material-icons prefix">account_circle</i>
            <input id="icon_prefix" type="text" class="validate" value={{ last_name }}>
            <label for="icon_prefix">Last Name</label>
          </div>
<!-- -Text Area to Input Last name --> 
          <div class="input-field col s6">
            <i class="material-icons prefix">phone</i>
            <input id="icon_telephone" type="tel" class="validate" value={{ phone }}>
            <label for="icon_telephone">Telephone</label>
          </div>
<!-- -Text Area to Input Address--> 
          <div class="input-field col s6">
            <i class="material-icons prefix">home</i>
            <input id="icon_home" type="text" class="validate" value={{ address }}>
            <label for="icon_home">Address</label>
          </div>
        </div>
      </form>
    </div>
<!-- Send button -->
    <button class="btn waves-effect waves-light" type="submit" name="action">Submit
        <i class="material-icons right">send</i>
      </button>
    </div> 
    <div class="container"> 
      <div class="card-panel teal lighten-7 grid-example col s12">
